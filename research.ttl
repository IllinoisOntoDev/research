###  http://www.courseweb.lis.illinois.edu/~jcolber2/OntoDev/research#Agent

:Agent rdf:type owl:Class ;
       
       owl:equivalentClass [ rdf:type owl:Restriction ;
                             owl:onProperty :hasAgentIdentification ;
                             owl:someValuesFrom :AgentIdentification
                           ] ;
       
       rdfs:subClassOf owl:Thing ;
       
       owl:disjointWith :Date ,
                        :Edition ,
                        :Identification ,
                        :Work ;
       
       rdfs:comment "An Agent is a person or group of persons who creates a Work and has Identification." .



###  http://www.courseweb.lis.illinois.edu/~jcolber2/OntoDev/research#AgentIdentification

:AgentIdentification rdf:type owl:Class ;
                     
                     owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty :identifiesAgent ;
                                           owl:someValuesFrom :Agent
                                         ] ;
                     
                     rdfs:subClassOf :Identification ;
                     
                     owl:disjointWith :WorkIdentification .



