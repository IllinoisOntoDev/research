###  http://www.courseweb.lis.illinois.edu/~jcolber2/OntoDev/research#Work

:Work rdf:type owl:Class ;
      
      owl:equivalentClass [ rdf:type owl:Restriction ;
                            owl:onProperty :hasAuthor ;
                            owl:someValuesFrom :Agent
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasDate ;
                            owl:someValuesFrom :Date
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasWorkIdentification ;
                            owl:someValuesFrom :WorkIdentification
                          ] ;
      
      rdfs:subClassOf owl:Thing ;
      
      rdfs:comment "I use Work in the FRBR WEMI sense, in that it is any distinct artistic or intellectual creation." .



