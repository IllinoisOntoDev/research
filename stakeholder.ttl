#################################################################
#
#    Classes
#
#################################################################

 <http://courseweb.lis.illinois.edu/~broadwy2/OntoDev/research#Deliverable> rdf:type owl:Class .

<http://courseweb.lis.illinois.edu/~broadwy2/OntoDev/research#Funder> rdf:type owl:Class .

<http://courseweb.lis.illinois.edu/~broadwy2/OntoDev/research#Stakeholder> a owl:Class ;
  rdfs:subClassOf :Deliverable .

<http://courseweb.lis.illinois.edu/~broadwy2/OntoDev/research#Committment> a owl:Class ;
  rdfs:subClassOf :Deliverable .

<http://courseweb.lis.illinois.edu/~broadwy2/OntoDev/research#Date> 
a owl:Class ;
  rdfs:subClassOf :Deliverable .

<http://courseweb.lis.illinois.edu/~broadwy2/OntoDev/research#Publication> a owl:Class ;
  rdfs:subClassOf :Deliverable .
<http://courseweb.lis.illinois.edu/~broadwy2/OntoDev/research#intellectualProperty> a owl:Class ;
  rdfs:subClassOf :Publication .

<http://courseweb.lis.illinois.edu/~broadwy2/OntoDev/research#Resource> a owl:Class ;
  rdfs:subClassOf :Deliverable .


# http://courseweb.lis.illinois.edu/~broadwy2/OntoDev/research#Funder

:Funder a owl:Restriction ;
  owl:onProperty :partOf ;
  owl:someValuesFrom :Stakeholder .

:Funder rdfs:subClassOf od:HumanAgent . #done#


#################################################################
#
#    Properties
#
#################################################################

<http://courseweb.lis.illinois.edu/~broadwy2/OntoDev/research#creatorOf> a owl:ObjectProperty ;
  rdfs:domain :Agent ;
  rdfs:range :Deliverable .  #done#

<http://courseweb.lis.illinois.edu/~broadwy2/OntoDev/research#partOf> a owl:ObjectProperty ;
  rdfs:domain :Funder ;
  rdfs:range :Stakeholder . #done#

<http://courseweb.lis.illinois.edu/~broadwy2/OntoDev/research#Contains> a owl:ObjectProperty ;
  rdfs:domain :dataSet ;
  rdfs:range :intellectualProperty .  #done#



