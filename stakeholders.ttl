@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix onto: <http://courseweb.lis.illinois.edu/~broadwy2/stakeholders/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dctype: <http://purl.org/dc/dcmitype/> .
@prefix dcterms: <http://purl.org/dc/terms/> .

#################################################################
#
#    Classes
#
#################################################################


<http://courseweb.lis.illinois.edu/~broadwy2/stakeholders/#Funder> rdf:type owl:Class .

<http://courseweb.lis.illinois.edu/~broadwy2/stakeholders/#Stakeholder> a owl:Class ;
  rdfs:subClassOf :Funder .

<http://courseweb.lis.illinois.edu/~broadwy2/stakeholders/#Deliverable> rdf:type owl:Class .

<http://courseweb.lis.illinois.edu/~broadwy2/stakeholders/#dataSet> a owl:Class ;
  rdfs:subClassOf :Deliverable .

<http://courseweb.lis.illinois.edu/~broadwy2/stakeholders/#Publication> a owl:Class ;
  rdfs:subClassOf :Deliverable .

<http://courseweb.lis.illinois.edu/~broadwy2/stakeholders/#Report> a owl:Class ;
  rdfs:subClassOf :Deliverable .

<http://courseweb.lis.illinois.edu/~broadwy2/stakeholders/#Proposal> rdf:type owl:Class .


#################################################################
#
#    Properties 
#
#################################################################

<http://courseweb.lis.illinois.edu/~broadwy2/stakeholders/#creatorOf> a owl:ObjectProperty ;
  rdfs:domain od:Agent ;
  rdfs:range od:Proposal .  

<http://courseweb.lis.illinois.edu/~broadwy2/stakeholders/#deliveredItem> a owl:ObjectProperty ;
  rdfs:domain od:myDeliverable ;
  rdfs:range od:myReport .  

 <http://courseweb.lis.illinois.edu/~broadwy2/stakeholders/#deliveredIn> a owl:ObjectProperty ;
:deliveredIn a owl:ObjectProperty ;
	owl:inverseOf :deliveredItem.

<http://courseweb.lis.illinois.edu/~broadwy2/stakeholders/#deliveredTo>
od:deliveredTo owl:ObjectProperty . 

<http://courseweb.lis.illinois.edu/~broadwy2/stakeholders/#reciepentOf>
:recipentOf a owl:ObjectProperty ;
	owl:inverseOf :deliveredTo . 
  
#################################################################
#
#    Individuals 
#
#################################################################

<http://courseweb.lis.illinois.edu/~broadwy2/stakeholders/#myProposal>
onto:myProposal rdf:type owl:NamedIndividual .

<http://courseweb.lis.illinois.edu/~broadwy2/stakeholders/#myFunder
onto:myFunder rdf:type owl:NamedIndividual .


<http://courseweb.lis.illinois.edu/~broadwy2/stakeholders/#myReport>
onto:myReport rdf:type owl:NamedIndividual ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty onto:deliveredIn ;
                          owl:someValuesFrom onto:Report
                        ] ;
               
               onto:deliveredIn onto:myDeliverable .

<ttp://courseweb.lis.illinois.edu/~broadwy2/stakeholders/#myDeliverable>
onto:myDeliverable rdf:type owl:NamedIndividual ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty onto:deliveredItem ;
                           owl:someValuesFrom onto:Deliverable
                         ] ;
                
                onto:deliveredItem onto:myReport .




